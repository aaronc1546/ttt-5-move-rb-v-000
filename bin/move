#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here
puts "Welcome to Tic Tac Toe!"
board = [" "," "," "," "," "," "," "," "," "]
puts "Where would you like to go?"

input = gets.strip
index = input_to_index(input)

def move(array, index, val = 'X')
  array[index] = val
end

move(board, index)

display_board(board)